<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chun.whefe">

    <uses-permission android:name="android.permission.RESTART_PACKAGES" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignupActivity" />
        <activity
            android:name=".MapActivity"
            android:label="MapActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".ListActivity"
           />
        <activity android:name=".InfoActivity" />
        <activity android:name=".OrderActivity" />
        <activity android:name=".CafemenuActivity" />
        <activity android:name=".OrderListActivity" />
        <activity android:name=".OrderList2Activity" />
        <activity android:name=".ShoppingListActivity" />
        <activity android:name=".PaymentActivity" />
        <activity android:name=".CouponActivity" />
        <activity android:name=".CoupListActivity" />
        <activity
            android:name=".NavActivity"
            android:label="@string/title_activity_nav"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>